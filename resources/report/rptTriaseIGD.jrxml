	<queryString>
		<![CDATA[SELECT
    ti.no_rawat, p.no_rkm_medis, p.nm_pasien, DATE_FORMAT(p.tgl_lahir, '%d-%m-%Y') AS tgllahir, p.jk,
    ti.tgl_kunjungan AS tanggal, ti.cara_masuk, ti.alat_transportasi AS kendaraan, ti.alasan_kedatangan,
    ti.keterangan_kedatangan, mk.macam_kasus AS kasus, tp.keluhan_utama, ti.tekanan_darah AS td,
    ti.nadi, ti.pernapasan AS napas, ti.suhu AS temperatur, ti.saturasi_o2 AS saturasi, ti.nyeri,
    ti.bb, ti.crt, ti.gds, ti.riwayat_alergi, ti.riwayat_penyakit, ti.id_pasien_khusus,
    tp.kebutuhan_khusus, ti.kebutuhan_restrain, tp.catatan, tp.plan, pg.nama AS petgas
FROM data_triase_igd ti
    LEFT JOIN data_triase_igdprimer tp ON ti.no_rawat = tp.no_rawat
    INNER JOIN reg_periksa rp ON rp.no_rawat = ti.no_rawat
    INNER JOIN pasien p ON p.no_rkm_medis = rp.no_rkm_medis
    LEFT JOIN master_triase_macam_kasus mk ON ti.kode_kasus = mk.kode_kasus
    INNER JOIN pegawai pg ON pg.nik = tp.nik
    WHERE ti.no_rawat = '2025/09/09/000001']]>
	</queryString>
